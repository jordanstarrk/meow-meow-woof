{
  "manifest_version": 3,
  "name": "Meow Meow Woof",
  "short_name": "meow meow woof",
  "version": "4.1.3",
  "description": "Replace your new tab page with cat videos. No ads!",

  "icons": {
    "16": "/src/assets/images/16cat.png",
    "48": "/src/assets/images/48cat.png",
    "128": "/src/assets/images/128cat.png"
  },

  "action": {
    "default_icon": "/src/assets/images/cat.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["activeTab"],

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },

  "incognito": "split",

  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx"
}
